# Configuración de rate limiting
RATE_LIMIT_WINDOW=10*60*1000    # 10 minutos
RATE_LIMIT_CITY=5*60*1000		# 5 minutos
RATE_LIMIT_MAX=25               # 25 peticiones por IP
REDIS_TTL=43200                 # 12 horas en segundos