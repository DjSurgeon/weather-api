# Puerto del servidor
PORT=

# Clave API de Visual Crossing
API_KEY=

# URL de conexión a Redis
CLIENT_URL=http://localhost:3000
REDIS_URL=redis://localhost:6379

# Configuración de caché (12 horas en segundos)
